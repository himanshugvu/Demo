		int num=948,n=0;
		while(num/10!=0){
			n = getNum(num); 
		if(n>=900 && n<1000){
			
		}else if(n>=500 && n<900){
			
		}else if(n>=400 && n<500){
			
		}else if(n>=100 && n<400){
			
		}else if(n>=90 && n<100){
			
		}else if(n>=50 && n<90){
			
		}else if(n>=40 && n<50){
			
		}else if(n>=10 && n<40){
			
		}else if(n>=9 && n<10){
			
		}else if(n>=5 && n<9){
			
		}else if(n>=4 && n<5){
			
		}else {
			
		}
		//num=num-
		}

	}
	public static int getNum(Integer num){
		
		return (int)(num-(num/Math.pow(10, num.SIZE+1)));
		
	}
}
