
public class MergeArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[] a = {0,3,6,7};
		int[] b = {1,2,3,4};
		int[] o = new int[a.length*2];
		int a1=0,b1=0;
		for(int i=0;i<b.length*2;i++){
			if(a[a1]<=b[b1]){
				o[i]=a[a1];
				a1++;
			}else{
				o[i]=b[b1];
				b1++;
			}
		}

		System.out.println(o);
	}

	
}
